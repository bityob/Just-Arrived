{
    "name": "Just Arrived",
    "version": "1.0",
    "description": "An extension to that shows only the unread tweets on Twitter",
    "manifest_version": 2,
    "permissions": ["storage"],
    "content_scripts": [{
        "matches": ["https://twitter.com/home"],
        "js": ["contentScript.js"]
    }],
    "icons": {
        "16": "icons/logo_16.png",
        "32": "icons/logo_32.png",
        "48": "icons/logo_48.png",
        "128": "icons/logo_128.png"
    }
}